@using QuanLyBanHangOnline.Models;
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Layout/_Layout.cshtml";
}
@{
    IEnumerable<SanPham> dsDT = ViewBag.listDT as IEnumerable<SanPham>;
    IEnumerable<SanPham> dsMTB = ViewBag.listMTB as IEnumerable<SanPham>;
    IEnumerable<SanPham> dsLT = ViewBag.listLT as IEnumerable<SanPham>;

}
<div class="container">
    <div class="content-top">
        <h1>Điện thoại mới nhất</h1>
        <div class="grid-in">
            @Html.Partial("~/Views/SanPham/SanPhamStyle1Partial.cshtml", dsDT);
        </div>
    </div>
    <div class="content-top">
        <h1>Máy tính bảng mới nhất</h1>
        <div class="grid-in">
            @Html.Partial("~/Views/SanPham/SanPhamStyle1Partial.cshtml", dsMTB);
        </div>
    </div>
    <div class="content-top-bottom">
        <h2>Máy tính mới nhất</h2>
        @Html.Partial("~/Views/SanPham/SanPhamStyle2Partial.cshtml", dsLT)
    </div>
</div>


